{{>menu}}
<div class='hero-image'>
  <div class='hero-text'>
    <h1>
      Ingresar
    </h1>
    <form onsubmit='restore(event)'>
      <input
        id='form-email'
        type='password'
        name='form-email'
        placeholder='email'
      />
      <input
        id='form-password'
        type='password'
        name='form-password'
        placeholder='password'
      />
      <input type='submit' value='Login' />
    </form>
  </div>
</div>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src='js/restorePassword.js'></script>
